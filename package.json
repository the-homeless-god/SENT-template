{
  "author": "Marat Zimnurov <zimit@mail.ru>",
  "dependencies": {
    "swagger-ui-express": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.14.5",
    "@babel/runtime": "^7.12.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.20",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cross-fetch": "^3.0.6",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^5.0.0",
    "fontello-cli": "^0.6.2",
    "helmet": "^4.2.0",
    "node-sass": "^5.0.0",
    "open": "^7.3.1",
    "rollup": "^2.36.1",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-svelte": "~6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "svelte": "^3.31.2",
    "svelte-check": "^1.1.26",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.6.1",
    "ts-loader": "^9.2.3",
    "typescript": "^4.1.3",
    "webpack": "^5.33.2"
  },
  "description": "Svelte Express Node Template",
  "homepage": "https://github.com/Zimtir/SENT-template",
  "keywords": [
    "actions",
    "commitlint",
    "cypress",
    "docker",
    "docker-compose",
    "eslint",
    "express",
    "fontello",
    "husky",
    "node",
    "sapper",
    "scss",
    "stylelint",
    "svelte",
    "prettier",
    "typescript"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/**/*.(js|ts|json)": [
      "npm run format-and-fix",
      "git add"
    ],
    "src/**/*.(svelte)": [
      "npm run svelte:check"
    ],
    "src/**/*.scss": [
      "npm run stylelint:scss:fix",
      "git add"
    ],
    "src/**/*.css": [
      "npm run stylelint:css:fix",
      "git add"
    ],
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "name": "sent-template",
  "nyc": {
    "cache": false,
    "extension": [
      ".js",
      ".ts",
      ".svelte"
    ],
    "all": true
  },
  "scripts": {
    "build": "sapper build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "depcheck": "npx depcheck",
    "dev": "sapper dev",
    "export": "sapper export --legacy",
    "eslint": "eslint --cache --ext .js,.ts, src",
    "eslint:dump": "eslint --print-config ./.eslintrc.json",
    "eslint:fix": "eslint --fix ./*.js ./src/*.ts ./src/**/*.ts --max-warnings=0",
    "fix": "npm run prettier && npm run eslint:fix && npm run stylelint:fix && npm run svelte:check",
    "format-and-fix": "npm run prettier && npm run eslint --fix",
    "fontello": "fontello-cli install --config ./fontello.config.json --css ./public/assets/css --font ./public/assets/font",
    "install:dev": "npm install --no-optional",
    "install:dev:full": "npm install",
    "install:prod:full": "npm ci",
    "install:prod": "npm ci --no-optional",
    "prettier": "prettier --write ./src/**",
    "prettier:root": "npm run prettier:root:json && npm run prettier:root:js",
    "prettier:root:js": "prettier --write ./*.js",
    "prettier:root:json": "prettier --write ./*.json",
    "start": "node __sapper__/build",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "stylelint": "npm run stylelint:css && npm run stylelint:scss",
    "stylelint:fix": "npm run stylelint:css:fix && npm run stylelint:scss:fix",
    "stylelint:css": "stylelint src/**/*.css --syntax css --allow-empty-input",
    "stylelint:scss": "stylelint src/**/*.scss --syntax scss --allow-empty-input",
    "stylelint:css:fix": "npm run stylelint:css --fix",
    "stylelint:scss:fix": "npm run stylelint:scss --fix",
    "svelte:check": "svelte-check --ignore='src/node_modules'",
    "test:dev": "npm run test:headless:dev && npm run test:coverage",
    "test:prod": "npm run test:headless:prod && npm run test:coverage",
    "test:coverage": "nyc report --reporter=text-summary",
    "test:docker": "./node_modules/.bin/cypress run",
    "test:headless:dev": "start-server-and-test dev http://localhost:3000 test:headless",
    "test:headless:prod": "start-server-and-test start http://localhost:3000 test:headless",
    "test:headless": "cypress run",
    "test:ui": "cypress open",
    "test:ui:dev": "start-server-and-test dev http://localhost:3000 test:ui",
    "test:ui:prod": "start-server-and-test start http://localhost:3000 test:ui",
    "update:install": "npm update && npm install",
    "update:pull": "npm outdated"
  },
  "optionalDependencies": {
    "@bahmutov/cy-rollup": "^2.0.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^11.0.0",
    "@cypress/code-coverage": "^3.9.1",
    "@storybook/addon-a11y": "^6.1.10",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-knobs": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/addon-storysource": "^6.1.10",
    "@storybook/addon-viewport": "^6.1.11",
    "@storybook/svelte": "^6.1.10",
    "@types/cypress": "^1.1.3",
    "@types/mocha": "^8.0.4",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress": "^5.6.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^3.1.2",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^1.4.2",
    "rollup-plugin-istanbul": "^3.0.0",
    "start-server-and-test": "^1.12.1",
    "source-map-support": "^0.5.19",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zimtir/SENT-template.git"
  },
  "version": "0.4.6"
}
